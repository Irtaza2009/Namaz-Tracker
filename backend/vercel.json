{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://namaz-tracker.irtaza.xyz", "http://localhost:8081",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "59 23 * * *"
    }
  ]
}
